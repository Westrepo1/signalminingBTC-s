			msg.html('<div class="alert alert-danger">Invalid amount</div>');
		} else if (amt.val() < 50) {
			msg.html('<div class="alert alert-danger">Please select an amount that is no less than 50 USD</div>');
		} else if (amt.val() >= 510000000) {
